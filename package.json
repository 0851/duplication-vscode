{
	"name": "duplication-vscode",
	"displayName": "duplication-vscode",
	"description": "检查重复代码",
	"version": "0.1.0",
	"publisher": "0851",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "Duplication Configuration",
				"properties": {
					"duplication.ignore": {
						"type": "array",
						"default": [
							"node_modules/**/*",
							"bower_components",
							"jspm_packages",
							"web_modules",
							".cache",
							".history",
							".yarn/cache",
							".vscode-test",
							".idea",
							".vscode",
							".git",
							"out",
							"dist",
							"build",
							"logs"
						],
						"items": {
							"type": "string"
						},
						"description": "忽略文件列表",
						"scope": "window"
					},
					"duplication.debounceWait": {
						"type": "number",
						"default": 500,
						"description": "节流调用等待时间 -> 500ms",
						"scope": "window"
					},
					"duplication.minTokens": {
						"type": "number",
						"default": 39,
						"description": "最小匹配分词 -> 39",
						"scope": "window"
					},
					"duplication.maxSize": {
						"type": "string",
						"default": "100kb",
						"description": "最大文件大小 -> 100Kb",
						"scope": "window"
					},
					"duplication.debug": {
						"type": "boolean",
						"default": false,
						"description": "debug",
						"scope": "window"
					},
					"duplication.severity": {
						"type": "number",
						"enum": [
							0,
							1,
							2
						],
						"default": 2,
						"description": "提示等级 0,1,2",
						"scope": "window"
					}
				}
			}
		],
		"commands": [
			{
				"command": "extension.duplication",
				"title": "Show Duplication"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"lint": "eslint src --ext ts",
		"compile": "webpack --mode production",
		"watch": "webpack --mode development --watch --info-verbosity verbose",
		"test": "mocha -r ts-node/register ./src/**/*.test.ts"
	},
	"license": "MIT",
	"dependencies": {
		"chokidar": "^3.4.0",
		"vscode-languageclient": "^6.1.3",
		"vscode-languageserver": "^6.1.1",
		"vscode-languageserver-textdocument": "^1.0.1"
	},
	"devDependencies": {
		"@babel/types": "^7.11.5",
		"@types/bytes": "^3.1.0",
		"@types/lodash-es": "^4.17.3",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.15",
		"@types/pify": "^3.0.2",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"bytes": "^3.1.0",
		"eslint": "^6.8.0",
		"eslint-loader": "^4.0.2",
		"eventemitter3": "^4.0.7",
		"globby": "^11.0.1",
		"lodash-es": "^4.17.15",
		"mocha": "^7.1.2",
		"pify": "^5.0.0",
		"ts-loader": "^8.0.0",
		"ts-node": "^8.10.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/0851/duplication-vscode.git"
	},
	"bugs": {
		"url": "https://github.com/0851/duplication-vscode/issues"
	},
	"homepage": "https://github.com/0851/duplication-vscode#readme"
}
